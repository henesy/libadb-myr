use std
use adb

const main = {
	match adb.open("./test.adb")
	| `std.Ok	f:
		std.put("opened.\n")

		match adb.parse(f)
		| `std.Ok	db:
			std.put("parsed.\n")

		| `std.Err	e:
			std.fatal("err: could not parse file: {}\n", e)
		;;

	| `std.Err	e:
		std.fatal("err: could not open ndb file: {}\n", e)
	;;
}
